<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>OpMatT</servlet-name>
        <servlet-class>OperatiiMatematiceTraditional</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>OpMatT</servlet-name>
        <url-pattern>/operatiiMatematiceTraditional</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>OpMatAjax</servlet-name>
        <servlet-class>OperatiiMatematiceAjax</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>OpMatAjax</servlet-name>
        <url-pattern>/operatiiMatematiceAjax</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Posteaza</servlet-name>
        <servlet-class>Posteaza</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Posteaza</servlet-name>
        <url-pattern>/posteaza</url-pattern>
    </servlet-mapping>


</web-app>
